#include <stdio.h>
int main()
{
    int n;
    int max = 0;
    printf("Nhap chieu dai cua mang: ");
    scanf("%d",&n);
    printf("Nhap cac phan tu cua mang: \n");
    int arr[n];
    int arr2[1000]={0};
    for(int i = 0; i <n;i++){
        printf("arr[%d] = ",i);
        scanf("%d",&arr[i]);
    }
    for(int i = 0 ; i < n; i++){
        max = arr[i]>max?arr[i]:max;
    }
    printf("Cac so con thieu la: ");
    for(int i = 0 ; i < n ; i++){
        arr2[arr[i]]=arr[i];
    }
    for(int i = 1;i<max;i++){
        if(arr2[i]==0){
            printf("%d ",i);
        }
    }
}
