#include <stdio.h>
void SapXep(int n,int arr[n]){
    for(int i = 0; i < n;i++){
        for(int j = i+1; j <n;j++){
            if(arr[i]>arr[j]){
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
    for(int i = 0; i <n;i++){
        printf("%d ",arr[i]);
    }
}
int main()
{
    int n;
    printf("Nhap chieu dai cua mang: ");
    scanf("%d",&n);
    printf("Nhap cac phan tu cua mang: \n");
    int arr[n];
    int arr2[1000]={0};
    for(int i = 0; i <n;i++){
        printf("arr[%d] = ",i);
        scanf("%d",&arr[i]);
    }
    SapXep(n,arr);
    printf("\nCac so con thieu la: ");
    int max = arr[n-1];
    
    for(int i = 1 ; i <= max;i++){
        arr2[i]='n';
    }
    for(int i = 0 ; i < n ; i++){
        arr2[arr[i]]='y';
    }
    for(int i = 1;i<max;i++){
        if(arr2[i]=='n'){
            printf("%d ",i);
        }
    }
}
